{
  "name": "storyteller",
  "license": "UNLICENSED",
  "repository": {
    "type": "git",
    "url": "https://github.com/socrata/storyteller.git"
  },
  "scripts": {
    "lint": "eslint --ignore-path .eslintignore -- app/assets/javascripts spec/karma",
    "test": "RAILS_ENV=karma node --max_old_space_size=4096 ./node_modules/.bin/karma start spec/karma/karma.conf.js",
    "test-watch": "RAILS_ENV=karma node --max_old_space_size=4096 ./node_modules/.bin/karma start spec/karma/karma.conf.js --singleRun false",
    "test-browser": "RAILS_ENV=karma node --max_old_space_size=4096 ./node_modules/.bin/karma start spec/karma/karma.conf.js --singleRun false --browsers Chrome",
    "webpack": "NODE_ENV=production node --max_old_space_size=4096 ./node_modules/.bin/webpack --config config/webpack.config.js --bail",
    "watch": "webpack-dev-server --config config/webpack.config.js",
    "build": "npm run webpack",
    "postinstall": "npm run webpack"
  },
  "homepage": "https://github.com/socrata/storyteller",
  "devDependencies": {
    "assets-webpack-plugin": "3.3.0",
    "babel-core": "6.10.4",
    "babel-loader": "6.2.2",
    "babel-plugin-rewire": "1.0.0",
    "babel-plugin-syntax-jsx": "6.3.13",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-preset-es2015": "6.3.13",
    "babel-preset-react": "6.16.0",
    "babel-preset-stage-3": "6.24.1",
    "chai": "4.1.2",
    "chroma-js": "0.5.6",
    "enzyme": "2.9.1",
    "es6-promise-polyfill": "1.2.0",
    "eslint": "2.10.2",
    "eslint-base": "2.2.0",
    "eslint-plugin-react": "5.2.2",
    "expose-loader": "0.7.1",
    "file-loader": "0.8.5",
    "imports-loader": "0.6.5",
    "json-loader": "0.5.4",
    "karma": "1.5.0",
    "karma-chrome-launcher": "0.1.12",
    "karma-cli": "0.0.4",
    "karma-firefox-launcher": "0.1.6",
    "karma-mocha": "1.3.0",
    "karma-mocha-reporter": "2.2.3",
    "karma-sourcemap-loader": "0.3.7",
    "karma-webpack": "1.7.0",
    "karma-phantomjs-launcher": "1.0.0",
    "mocha": "3.2.0",
    "pbf": "1.3.5",
    "phantomjs-prebuilt": "2.1.7",
    "raw-loader": "0.5.1",
    "rimraf": "2.5.2",
    "simple-statistics": "0.8.1",
    "sinon": "4.1.3",
    "url-loader": "0.5.7",
    "vector-tile": "1.3.0",
    "webpack": "1.13.1",
    "webpack-dev-server": "1.14.1",
    "wellknown": "0.5.0"
  },
  "eslintConfig": {
    "extends": "./node_modules/eslint-base/.eslintrc.json",
    "env": {
      "mocha": true
    },
    "plugins": [
      "react"
    ],
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
        "impliedStrict": true,
        "jsx": true
      }
    },
    "rules": {
      "indent": [
        2,
        2,
        {
          "SwitchCase": 1
        }
      ],
      "dot-notation": [
        1,
        {
          "allowKeywords": true,
          "allowPattern": "^[a-z]+(_[a-z]+)+$"
        }
      ],
      "quote-props": [
        2,
        "as-needed",
        {
          "keywords": false,
          "unnecessary": false
        }
      ],
      "react/jsx-uses-vars": 1,
      "react/jsx-uses-react": 1
    },
    "globals": {
      "assert": true,
      "expect": true,
      "sinon": true,
      "_": true,
      "storyteller": true,
      "Promise": true
    }
  },
  "dependencies": {
    "airbrake-js": "0.9.3",
    "babel-polyfill": "6.16.0",
    "babel-polyfill-safe": "1.0.1",
    "bignumber.js": "4.1.0",
    "bourbon": "4.2.7",
    "bowser": "1.7.0",
    "browser-cookies": "1.1.0",
    "classnames": "2.2.5",
    "cldr-core": "31.0.1",
    "cldrpluralruleparser": "1.1.3",
    "css-loader": "0.23.1",
    "d3": "3.5.16",
    "dompurify": "0.8.3",
    "dotdotdot": "1.7.0",
    "flux": "3.1.2",
    "i18n-js": "1.0.0",
    "jquery": "2.2.4",
    "jquery-sidebar": "3.3.2",
    "jquery-ujs": "1.2.0",
    "js-cookie": "2.1.2",
    "jstz": "1.0.10",
    "leaflet": "0.7.7",
    "lodash": "4.14.2",
    "@socrata/mapbox-gl": "0.0.5",
    "@mapbox/mapbox-gl-geocoder": "2.1.1",
    "mixpanel-browser": "2.11.0",
    "modularscale-sass": "2.1.1",
    "moment-timezone": "0.5.13",
    "node-sass": "3.7.0",
    "normalize-scss": "4.1.0",
    "phoenix": "1.2.1",
    "prop-types": "15.5.10",
    "react": "15.6.1",
    "react-addons-css-transition-group": "15.4.2",
    "react-css-modules": "3.7.10",
    "react-datepicker": "0.39.0",
    "react-dom": "15.6.1",
    "react-image-crop": "1.0.0-rc3",
    "react-input-range": "0.9.2",
    "react-linkify": "0.2.1",
    "react-motion": "0.5.0",
    "react-redux": "5.0.6",
    "react-router": "3.0.5",
    "react-test-renderer": "15.6.1",
    "react-transition-group": "1.2.0",
    "redux": "3.5.2",
    "redux-logger": "2.6.1",
    "redux-saga": "0.16.0",
    "redux-thunk": "2.1.0",
    "reselect": "2.5.3",
    "sass-loader": "4.1.1",
    "showdown": "git+https://github.com/socrata/showdown.git#openperformance-compat",
    "squire-rte": "git+https://github.com/socrata-platform/Squire.git#1.4.2-fixes",
    "style-loader": "0.18.2",
    "tether": "1.0.2",
    "tether-shepherd": "1.2.0",
    "text-encoding": "^0.6.4",
    "unidragger": "2.1.0",
    "unipointer": "2.1.0",
    "uuid": "3.2.1",
    "validator": "5.4.0",
    "velocity-animate": "1.3.2",
    "whatwg-fetch": "1.0.0",
    "yaml-loader": "0.5.0"
  }
}
