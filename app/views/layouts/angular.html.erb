<!doctype html>
<html>

  <head>

    <meta http-equiv="content-type" content="text/html;charset=utf-8" />

    <link type="text/css" media="all" rel="stylesheet" href="/webfonts/ss-air.css?<%= asset_revision_key %>"/>
    <link type="text/css" media="all" rel="stylesheet" href="/webfonts/socrata-apps/socrata-apps.css?<%= asset_revision_key %>"/>
    <link rel="shortcut icon" href="<%= theme_image_url(CurrentDomain.theme.images.favicon) %>" />

    <%= angular_theme_tag %>

    <%= rendered_stylesheet_tag 'bower-styles-all' %>
    <%= angular_stylesheet_tag %>
    <!--[if IE 9]><%= rendered_stylesheet_tag 'ie9' %><![endif]-->

    <%= yield :head %>

    <%- unless CurrentDomain.properties.typekit_id.blank? -%>
      <script type="text/javascript" src="//use.typekit.net/<%= CurrentDomain.properties.typekit_id %>.js"></script>
      <script type="text/javascript">try{Typekit.load();}catch(e){}</script>
    <%- end -%>

    <%= render_angular_config %>
    <%= render_angular_translations %>
    <%= render_user %>
    <%= render_metadata %>

  </head>

  <body ng-app="<%= angular_app %>">

    <% if @migration_metadata %>
      <div id="skip-links">
        <%= link_to 'Go to an accessible version of this page', get_alt_dataset_link(@migration_metadata[:obeId]) %>
      </div>
      <%= render_noscript(@migration_metadata[:obeId]) %>
    <% end %>

    <%= include_javascripts 'bower-all' %>
    <%= include_javascripts 'angular-jquery-extensions' %>
    <%= render_airbrake_shim %>
    <%= render_airbrake_notifier %>
    <%= angular_javascript_tag %>
    <%= include_webpack_bundle "angular/data-lens.js" %>

    <%= yield %>
  </body>


  <%= render_ga_tracking %>

</html>
