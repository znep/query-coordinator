{
  "extends": "../../../node_modules/eslint-base/.eslintrc.json",
  "rules": {
    "padded-blocks": 0,
    "import/default": "off",
    "dot-notation": ["error", {
      "allowKeywords": true,
      "allowPattern": "^[a-z]+(_[a-z]+)+$"
    }]
  },
  "settings": {
    "import/ignore": [
      "\\.(scss|css)$"
    ]
  }
}
