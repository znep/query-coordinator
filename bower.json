{
  "name": "socrata-frontend",
  "version": "0.0.0",
  "authors": [
    "Socrata Engineering <engineering@socrata.com>"
  ],
  "description": "Socrata's default UX.",
  "license": "all rights reserved",
  "homepage": "http://socrata.com",
  "private": true,
  "dependencies": {
    "angular": "1.3",
    "angular-elastic": "2.4.2",
    "angular-mocks": "1.3",
    "angular-rx": "0.0.14",
    "angular-sanitize": "1.3",
    "angular-ui-router": "0.2.10",
    "chroma-js": "0.5.6",
    "d3": "3.4.6",
    "flexgrid": "ptb/flexgrid",
    "javascript-detect-element-resize": "sdecima/javascript-detect-element-resize#0.5.3",
    "jjv": "1.0.2",
    "jquery": "2.1.1",
    "jQuery.dotdotdot": "1.7.4",
    "js-polyfills": "0.1.2",
    "leaflet": "0.7.3",
    "lodash-compat": "3.7",
    "moment": "2.7.0",
    "pbf": "0.0.3",
    "react": "0.13.3",
    "requestAnimationFrame-polyfill": "*",
    "rxjs": "2.5",
    "simple-statistics": "0.8.1",
    "socrata-utils": "git@github.com:socrata/frontend-utils.git#master",
    "socrata-visualizations": "git@github.com:socrata/frontend-visualizations.git#master",
    "vector-tile-js": "devTristan/vector-tile-js#0.1.1"
  },
  "install": {
    "path": {
      "css": "app/styles/bower",
      "js": "public/javascripts/bower",
      "png": "public/images/bower",
      "sass": "app/styles/bower"
    },
    "ignore": [
      "angular-mocks",
      "sinon-browser-only"
    ],
    "sources": {
      "angular": "bower_components/angular/angular.min.js",
      "flexgrid": [],
      "javascript-detect-element-resize": "bower_components/javascript-detect-element-resize/jquery.resize.js",
      "js-polyfills": "bower_components/js-polyfills/url.js",
      "leaflet": [
        "bower_components/leaflet/dist/leaflet.js",
        {
          "bower_components/leaflet/dist/leaflet.css": "../leaflet.css"
        }
      ],
      "lodash-compat": "bower_components/lodash-compat/lodash.js",
      "pbf": "bower_components/pbf/dist/pbf.min.js",
      "rxjs": [
        "bower_components/rxjs/dist/rx.js",
        "bower_components/rxjs/dist/rx.async.js",
        "bower_components/rxjs/dist/rx.aggregates.js",
        "bower_components/rxjs/dist/rx.time.js",
        "bower_components/rxjs/dist/rx.binding.js"
      ],
      "socrata-utils": [
        "bower_components/socrata-utils/socrata.utils.js"
      ],
      "socrata-visualizations": [
        "bower_components/socrata-visualizations/socrata.visualizations.pbf.js",
        "bower_components/socrata-visualizations/socrata.visualizations.VectorTileManager.js",
        "bower_components/socrata-visualizations/socrata.visualizations.Visualization.js",
        "bower_components/socrata-visualizations/socrata.visualizations.ColumnChart.js",
        "bower_components/socrata-visualizations/socrata.visualizations.FeatureMap.js",
        {
          "bower_components/socrata-visualizations/socrata.visualizations.columnChart.css": "../socrata.visualizations.columnChart.css",
          "bower_components/socrata-visualizations/socrata.visualizations.columnChart.css": "../socrata.visualizations.featureMap.css"
        }
      ],
      "vector-tile-js": "bower_components/vector-tile-js/dist/vectortile.min.js"
    }
  },
  "devDependencies": {
    "native-promise-only": "0.7.5-e",
    "sinon-browser-only": "1.9.0"
  }
}
