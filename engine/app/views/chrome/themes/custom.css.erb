<% cache(cache_key_for_site_chrome(get_site_chrome)) do %>
  <% unless prevent_font_render? %>
    <%== render(:partial => 'socrata_icons') %>
  <% end %>
  <% if params.key?(:debug_sass) %>
///////////////////////
//   Unparsed SASS   //
///////////////////////

<%== render(:partial => 'theme', :locals => { :custom_themes => get_site_chrome.styles }) %>
  <% else %>
<%== Sass::Engine.new(render(:partial => 'theme', :locals => { :custom_themes => get_site_chrome.styles }), sass_engine_options).render %>
  <% end %>
<% end %>
