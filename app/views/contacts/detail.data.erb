
<div class="header">
  <ul class="summaryTabs clearfix">
    <li class="summary active last">
      <div class="tabInner">
        <span>Summary</span>
      </div>
    </li>
  </ul>
</div>

<div class="infoContentOuter listPane active">
  <div class="infoContentInner">
    <div class="infoContentHeader clearfix">
      <h2 class="panelHeader <%= (@type == "contacts" || @type == "contacts and groups") ? "contactPanelHeader" : "groupPanelHeader" %>">
        <%= pluralize(@item_count, @type) %>
      </h2>
      <span class="selectPrompt">Select a specific <%= @type %> or <%= "#{@type}s" %> above for more information</span>
    </div>
    <div class="infoContent"></div>
  </div>
</div>