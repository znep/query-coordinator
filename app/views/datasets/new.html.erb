<%- page = 'screens.dataset_new' -%>
<% content_for :head do %>
  <%= rendered_stylesheet_tag 'dataset-new' %>
  <title><%= t("#{page}.title") %> | <%= CurrentDomain.strings.site_title %></title>
<% end %>

<% content_for :skip_links do %>
  <a href="<%= upload_datasets_path %>"><%= t("#{page}.upload_accessible") %></a>
<% end %>

<div id="app"></div>

<%= render_import_wizard_server_config %>
<%= render_import_wizard_translations %>
<%= render_import_wizard_custom_metadata %>
<%= render_import_wizard_importable_types %>
<%= render_import_wizard_enabled_modules %>
<%= render_import_wizard_dataset_categories %>
<%= render_import_wizard_view(@view) %>

<% content_for :js_footer do %>
  <%= render_import_wizard_licenses %>
<% end %>

<%= include_webpack_bundle 'import-wizard/main.js' %>
