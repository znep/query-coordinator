{
  "name": "chrome",
  "version": "0.0.0",
  "description": "The \"Chrome\" is the header (including navigation bar) and footer of a customer site. This gem exists so that our various services/pages (catalogs, storyteller, data_lens, etc.) can easily render the same html for these components.",
  "scripts": {
    "check-dependencies": "check-dependencies",
    "lint": "eslint -c package.json ../**/*.js"
  },
  "author": "Socrata Engineering <engineering@socrata.com>",
  "license": "UNLICENSED",
  "dependencies": {
    "socrata-notifications": "^1.0.6",
    "socrata-autocomplete": "^1.0.8"
  },
  "devDependencies": {
    "check-dependencies": "1.0.1",
    "eslint": "2.3.0",
    "eslint-base": "0.2.3"
  },
  "eslintConfig": {
    "extends": "./node_modules/eslint-base/.eslintrc.json",
    "rules": {
      "dot-notation": [
        1,
        {
          "allowKeywords": true,
          "allowPattern": "^[a-z]+(_[a-z]+)+$"
        }
      ]
    },
    "globals": {
      "$": true,
      "_": true,
      "jQuery": true,
      "L": true,
      "moment": true,
      "pbf": true,
      "socrata": true,
      "ss": true,
      "require": true,
      "module": true
    }
  }
}
