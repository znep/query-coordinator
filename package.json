{
  "name": "socrata-frontend",
  "version": "0.0.0",
  "description": "Socrata's default UX.",
  "main": "",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint"
  },
  "repository": {
    "type": "git",
    "url": "git@git.socrata.com:frontend.git"
  },
  "author": "Socrata Engineering <engineering@socrata.com>",
  "license": "all rights reserved",
  "devDependencies": {
    "chai": "~1.9.1",
    "chai-as-promised": "~4.1.1",
    "eslint": "^0.24.0",
    "http-proxy": "^1.1.6",
    "karma": "~0.12.16",
    "karma-chai": "~0.1.0",
    "karma-chai-plugins": "~0.2.4",
    "karma-chrome-launcher": "~0.1.3",
    "karma-cli": "0.0.4",
    "karma-coverage": "~0.2.1",
    "karma-firefox-launcher": "~0.1.3",
    "karma-mocha": "~0.1.3",
    "karma-mocha-reporter": "~0.2.7",
    "karma-ng-html2js-preprocessor": "~0.1",
    "karma-phantomjs-launcher": "^0.2.0",
    "karma-sauce-launcher": "^0.2.10",
    "lodash": "^2.4.1",
    "mocha": "~1.18.2",
    "phantomjs": "^1.9.17",
    "sprintf": "^0.1.4",
    "underscore": "^1.6.0",
    "underscore.string": "^2.3.3"
  },
  "eslintConfig": {
    "env": {
      "browser": true
    },
    "rules": {
      "brace-style": [2, "1tbs", {"allowSingleLine": true}],
      "camelcase": 1,
      "consistent-return": 1,
      "dot-location": [1, "object"],
      "dot-notation": [1, {"allowKeywords": false}],
      "eol-last": 1,
      "indent": [1, 2],
      "new-cap": 1,
      "no-console": 0,
      "no-extra-semi": 1,
      "no-shadow": [2, {"hoist": "all"}],
      "no-spaced-func": 2,
      "no-trailing-spaces": 2,
      "no-underscore-dangle": 0,
      "no-unused-vars": 1,
      "no-use-before-define": 0,
      "quotes": [1, "single"],
      "semi": [2, "always"],
      "space-after-keywords": [2, "always"],
      "space-before-blocks": [2, "always"],
      "space-infix-ops": 2,
      "strict": 2
    },
    "globals": {
      "$": true,
      "_": true,
      "angular": true,
      "blist": true,
      "chroma": true,
      "d3": true,
      "datasetMetadata": true,
      "jQuery": true,
      "L": true,
      "Modernizr": true,
      "moment": true,
      "pageMetadata": true,
      "pbf": true,
      "Rx": true,
      "ss": true,
      "socrata": true,
      "VBArray": true
    }
  }
}
