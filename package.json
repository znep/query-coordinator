{
  "name": "socrata-utils",
  "version": "3.0.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/socrata/frontend-utils.git"
  },
  "publishConfig": {
    "registry": "https://socrata.artifactoryonline.com/socrata/api/npm/npm-local"
  },
  "scripts": {
    "build": "webpack",
    "prebuild": "rimraf dist",
    "prepublish": "webpack",
    "lint": "eslint -c package.json src/*.js",
    "test": "karma start karma.conf.js --singleRun true",
    "version-bump-check": "node tools/version-bump-check.js",
    "watch": "karma start karma.conf.js -v --singleRun false"
  },
  "homepage": "https://github.com/socrata/frontend-utils",
  "dependencies": {
    "es6-promise-polyfill": "1.2.0"
  },
  "main": "dist/socrata-utils.js",
  "devDependencies": {
    "chai": "3.5.0",
    "eslint": "2.3.0",
    "eslint-base": "0.2.3",
    "jquery": "2.2.4",
    "karma": "0.13.22",
    "karma-chai": "0.1.0",
    "karma-chrome-launcher": "0.2.3",
    "karma-coverage": "0.5.5",
    "karma-mocha": "0.2.2",
    "karma-phantomjs-launcher": "0.2.3",
    "karma-phantomjs-shim": "1.4.0",
    "karma-sinon": "1.0.5",
    "karma-sourcemap-loader": "0.3.7",
    "karma-webpack": "1.7.0",
    "lodash": "4.13.1",
    "mocha": "2.5.3",
    "phantomjs": "1.9.20",
    "rimraf": "2.5.2",
    "semver": "5.1.0",
    "shelljs": "0.6.0",
    "sinon": "1.17.4",
    "webpack": "1.13.1"
  },
  "eslintConfig": {
    "extends": "./node_modules/eslint-base/.eslintrc.json",
    "globals": {
      "$": true,
      "_": true,
      "jQuery": true,
      "L": true,
      "moment": true,
      "pbf": true,
      "socrata": true,
      "ss": true,
      "require": true,
      "module": true
    }
  },
  "files": [
    "dist"
  ]
}
