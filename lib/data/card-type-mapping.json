{
  "map": {
    "boolean": [
      { "type": "column" }
    ],
    "floating_timestamp": [
      { "type": "timeline" }
    ],
    "fixed_timestamp": [
      { "type": "timeline" }
    ],
    "geo_entity": [
      { "type": "feature" }
    ],
    "money": [
      {
        "type": "column",
        "defaultIf": "isLowCardinality"
      },
      {
        "type": "numberHistogram",
        "defaultIf": "isHighCardinality"
      },
      { "type": "search" }
    ],
    "number": [
      {
        "type": "choropleth",
        "onlyIf": "isGeoregionComputed",
        "defaultIf": "isGeoregionComputed"
      },
      {
        "type": "column",
        "defaultIf": "isLowCardinality"
      },
      {
        "type": "numberHistogram",
        "defaultIf": "isHighCardinality"
      },
      { "type": "search" }
    ],
    "point": [
      { "type": "feature" }
    ],
    "text": [
      {
        "type": "column",
        "defaultIf": "isLowCardinality"
      },
      { "type": "search" }
    ],
    "*": [
      { "type": "table" }
    ]
  },
  "cardinality": {
    "min": 2,
    "threshold": 35,
    "default": 9007199254740992
  },
  "version": "0.3"
}
