{
  "extends": "../../../node_modules/eslint-base/.eslintrc-airbnb.json",
  "rules": {
    "arrow-body-style": 0,
    "block-spacing": 0,
    "default-case": 0,
    "import/no-mutable-exports": [2, "never"],
    "max-len": 0,
    "no-alert": 0,
    "no-confusing-arrow": 0,
    "no-use-before-define": 0,
    "object-curly-spacing": 0,
    "padded-blocks": 0,
    "prefer-template": 0,
    "space-in-parens": 0,
    "vars-on-top": 0,
    "dot-notation": ["error", {
      "allowKeywords": true
    }]
  }
}
