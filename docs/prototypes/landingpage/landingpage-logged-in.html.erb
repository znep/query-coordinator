    $('.lp-section.featured-content').removeClass('hidden');
  });


 // Enable the add button with a 'proper' stories link

setTimeout( function(){
  $('#story-url').on('input propertychange', function(){

    if ($(this).val() == 'link' ) {
      $('.url-error').hide();
      $('#story-modal .actual-content').show();
      $('#featured-item-1 .actual-content').show();
      $('.lp-section .featured-story').removeClass('hidden');
      $('.lp-section.featured-content').removeClass('hidden');
      $('#add-story-button').prop('disabled', false);
    }

    else {
      $('#story-modal .actual-content').hide();
      $('.url-error').show();
      $('#add-story-button').prop('disabled', true);
    }
  })
}, 4000);


 // Enable the add button with all filled out external info

setTimeout( function(){
  $('#ext-url-input').on('input propertychange', function(){

    if ($(this).val() == 'external link' ) {
      $('.url-error').hide();
      $('#featured-item-2 .actual-content').show();
      $('.lp-section .featured-ext').removeClass('hidden');
      $('.lp-section.featured-content').removeClass('hidden');
