{
  "name": "storyteller",
  "repository": {
    "type": "git",
    "url": "https://github.com/socrata/storyteller.git"
  },
  "scripts": {
    "lint": "./node_modules/.bin/eslint --ignore-path .eslintignore -- app/assets/javascripts spec/karma",
    "test": "RAILS_ENV=karma ./node_modules/.bin/karma start spec/karma/karma.conf.js",
    "test-watch": "RAILS_ENV=karma ./node_modules/.bin/karma start spec/karma/karma.conf.js --singleRun false",
    "webpack": "./node_modules/.bin/webpack --config config/webpack.config.js",
    "watch": "./node_modules/.bin/webpack-dev-server --config config/webpack.config.js",
    "build": "npm run webpack",
    "postinstall": "npm run webpack"
  },
  "homepage": "https://github.com/socrata/storyteller",
  "devDependencies": {
    "assets-webpack-plugin": "3.3.0",
    "babel-core": "6.4.5",
    "babel-eslint": "5.0.0",
    "babel-loader": "6.2.2",
    "babel-plugin-rewire": "1.0.0-beta-5",
    "babel-plugin-syntax-jsx": "6.3.13",
    "babel-preset-es2015": "6.3.13",
    "chai": "3.5.0",
    "eslint": "2.2.0",
    "eslint-base": "0.2.1",
    "expose-loader": "0.7.1",
    "imports-loader": "0.6.5",
    "json-loader": "0.5.4",
    "karma": "0.13.21",
    "karma-chai": "0.1.0",
    "karma-chrome-launcher": "0.2.2",
    "karma-mocha": "0.2.2",
    "karma-phantomjs-launcher": "1.0.0",
    "karma-sinon": "1.0.4",
    "karma-sourcemap-loader": "0.3.7",
    "karma-webpack": "1.7.0",
    "mocha": "2.4.5",
    "phantomjs-prebuilt": "2.1.4",
    "rewire": "2.5.1",
    "rimraf": "2.5.2",
    "sinon": "1.17.3",
    "webpack": "1.12.13",
    "webpack-dev-server": "1.14.1"
  },
  "eslintConfig": {
    "extends": "./node_modules/eslint-base/.eslintrc.json",
    "env": {
      "mocha": true
    },
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
        "impliedStrict": true
      }
    },
    "rules": {
      "dot-notation": [
        1,
        {
          "allowKeywords": true,
          "allowPattern": "^[a-z]+(_[a-z]+)+$"
        }
      ],
      "quote-props": [
        2,
        "as-needed",
        {
          "keywords": false,
          "unnecessary": false
        }
      ]
    },
    "globals": {
      "assert": true,
      "sinon": true,
      "Promise": true
    }
  },
  "dependencies": {
    "airbrake-js": "0.5.8",
    "d3": "3.5.14",
    "flux": "2.1.1",
    "jquery": "2.2.0",
    "jquery-sidebar": "3.3.2",
    "jquery-ujs": "1.2.0",
    "leaflet": "0.7.7",
    "lodash": "3.10.1",
    "moment": "2.11.2",
    "socrata-styleguide": "0.4.2",
    "socrata-utils": "1.2.0",
    "socrata-visualizations": "8.2.4",
    "squire-rte": "git+https://github.com/socrata-platform/Squire.git#1.4.2-fixes",
    "unidragger": "2.1.0",
    "unipointer": "2.1.0"
  }
}
