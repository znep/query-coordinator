{
  "extends": "airbnb",
  "rules": {
    "comma-dangle": [2, "never"],
    "import/no-mutable-exports": [2, "never"],
    "import/no-unresolved": 0,
    "no-use-before-define": [2, "nofunc"],
    "react/jsx-closing-bracket-location": [2, "after-props"],
    "react/prefer-es6-class": [2, "never"]
  }
}
