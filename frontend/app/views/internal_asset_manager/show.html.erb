<%= content_for :head do %>
  <%= rendered_stylesheet_tag('internal-asset-manager') %>
  <title><%= I18n.t('internal_asset_manager.title') %> | <%= get_site_title %></title>
<%- end %>

<% content_for :breadcrumb do %>
  <%= t('screens.admin.assets.asset_inventory') %>
<% end %>

<div class="catalog-results" id="internal-asset-manager-content"></div>

<div id="feedback-panel-content"></div>

<%= content_for :scripts do %>
  <script type="text/javascript" src="/javascripts/plugins/lodash.js"></script>
  <script type="text/javascript">var I18n = I18n || {};</script>

  <%= render_catalog_results_translations %>
  <%= render_catalog_results_session_data %>
  <%= render_catalog_results_server_config %>
  <%== render_catalog_results_initial_state %>
  <%= include_webpack_bundle 'internal-asset-manager/main' %>
<%- end %>
