<% unless render_admin_header? %>
  <% content_for(:relocated_content) do %><a name="above-content"></a><% end %>
  <div class="leftNavBox adminNavBox">
    <div class="hideButtonBox">
      <button type="button" class="hideButton close">
        <span class="icon">x</span>
      </button>
    </div>

    <ul class="navigationList adminNav">
      <%= admin_left_nav_link('site_analytics', true, '/admin/analytics') %>
      <%= admin_left_nav_link('users', user_can_see_users?, '/admin/users') %>
      <%= admin_left_nav_link('federation', user_can_see_federations?, '/admin/federations') %>
      <%= admin_left_nav_link('comment_moderation', user_can_see_comment_moderation?, '/admin/comment_moderation') %>
      <%= admin_left_nav_link('view_moderation', user_can_see_view_moderation?, '/admin/views') %>
      <%= admin_left_nav_link('sdp_templates', user_can_see_sdp_templates?, '/admin/sdp_templates') %>
      <%= admin_left_nav_link('datasets', user_can_see_asset_inventory? && !use_internal_asset_manager?, '/admin/datasets') %>
      <%= admin_left_nav_link('datasets', use_internal_asset_manager?, '/admin/assets') %>
      <%= admin_left_nav_link('georegions', can_view_georegions_admin?, '/admin/geo') %>
      <%= admin_left_nav_link('site_appearance', show_site_appearance_admin_panel?, '/admin/site_appearance') %>
      <%= admin_left_nav_link('home_page', user_can_see_home_page?, '/admin/home') %>
      <%= admin_left_nav_link('metadata', user_can_see_metadata?, '/admin/metadata') %>
      <%= admin_left_nav_link('approvals', user_can_review_approvals?, '/admin/approvals') %>
      <%= admin_left_nav_link('dataset_routing_and_approval', user_can_see_routing_approval?, '/admin/routing_approval') %>
      <%= admin_left_nav_link('activity_log', user_can_see_activity_feed?, '/admin/activity_feed') %>
      <%= admin_left_nav_link('internal', current_user.try(:is_superadmin?), '/internal') %>
      <%= admin_left_nav_link('stories', user_can_see_stories?, '/stories/admin/themes') %>
      <%= admin_left_nav_link('goals', user_can_see_goals?, '/admin/goals') %>
      <%= admin_left_nav_link('connectors', user_can_see_connectors?, '/admin/connectors') %>
      <%= admin_left_nav_link('feature_flags', current_user.try(:is_superadmin?), :controller => '/internal', :action => 'feature_flags', :domain_id => CurrentDomain.cname) %>
    </ul>
  </div>

  <a name="content" tabindex="-1"></a>

  <% content_for :js_footer do %>
    <%= include_webpack_bundle 'open-data/admin-collapse-nav.js' %>
  <% end %>
<% end %>
