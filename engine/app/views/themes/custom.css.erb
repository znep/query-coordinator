<% cache(cache_key_for_site_chrome(@site_chrome)) do %>
  <% if params.key?(:debug_sass) %>
///////////////////////
//   Unparsed SASS   //
///////////////////////

<%= raw render(:partial => 'theme', :locals => { :custom_themes => @custom_themes }) %>
  <% else %>
<%= raw Sass::Engine.new(render(:partial => 'theme', :locals => { :custom_themes => @custom_themes }), sass_engine_options).render %>
  <% end %>
<% end %>
