{
  "name": "storyteller",
  "license": "UNLICENSED",
  "repository": {
    "type": "git",
    "url": "https://github.com/socrata/storyteller.git"
  },
  "scripts": {
    "lint": "eslint --ignore-path .eslintignore -- app/assets/javascripts spec/karma",
    "test": "RAILS_ENV=karma karma start spec/karma/karma.conf.js",
    "test-watch": "RAILS_ENV=karma karma start spec/karma/karma.conf.js --singleRun false",
    "test-browser": "RAILS_ENV=karma karma start spec/karma/karma.conf.js --singleRun false --browsers Chrome",
    "webpack": "webpack --config config/webpack.config.js",
    "watch": "webpack-dev-server --config config/webpack.config.js",
    "build": "npm run webpack",
    "postinstall": "npm run webpack"
  },
  "homepage": "https://github.com/socrata/storyteller",
  "devDependencies": {
    "assets-webpack-plugin": "3.3.0",
    "babel-core": "6.4.5",
    "babel-eslint": "5.0.0",
    "babel-loader": "6.2.2",
    "babel-plugin-rewire": "1.0.0-beta-5",
    "babel-plugin-syntax-jsx": "6.3.13",
    "babel-polyfill": "6.16.0",
    "babel-preset-es2015": "6.3.13",
    "babel-preset-react": "6.16.0",
    "chai": "3.5.0",
    "eslint": "2.2.0",
    "eslint-base": "0.2.1",
    "expose-loader": "0.7.1",
    "imports-loader": "0.6.5",
    "json-loader": "0.5.4",
    "karma": "0.13.21",
    "karma-chai": "0.1.0",
    "karma-chrome-launcher": "0.2.2",
    "karma-mocha": "0.2.2",
    "karma-phantomjs-launcher": "1.0.0",
    "karma-sinon": "1.0.4",
    "karma-sourcemap-loader": "0.3.7",
    "karma-webpack": "1.7.0",
    "mocha": "2.4.5",
    "phantomjs-prebuilt": "2.1.7",
    "react-addons-test-utils": "15.3.2",
    "rewire": "2.5.1",
    "rimraf": "2.5.2",
    "sinon": "1.17.3",
    "webpack": "1.12.13",
    "webpack-dev-server": "1.14.1"
  },
  "eslintConfig": {
    "extends": "./node_modules/eslint-base/.eslintrc.json",
    "env": {
      "mocha": true
    },
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
        "impliedStrict": true,
        "jsx": true
      }
    },
    "rules": {
      "indent": [
        2,
        2,
        {
          "SwitchCase": 1
        }
      ],
      "dot-notation": [
        1,
        {
          "allowKeywords": true,
          "allowPattern": "^[a-z]+(_[a-z]+)+$"
        }
      ],
      "quote-props": [
        2,
        "as-needed",
        {
          "keywords": false,
          "unnecessary": false
        }
      ]
    },
    "globals": {
      "assert": true,
      "expect": true,
      "sinon": true,
      "_": true,
      "storyteller": true,
      "Promise": true
    }
  },
  "dependencies": {
    "airbrake-js": "0.5.8",
    "babel-polyfill": "6.16.0",
    "classnames": "2.2.5",
    "d3": "3.5.16",
    "dompurify": "0.8.3",
    "dotdotdot": "1.7.0",
    "flux": "2.1.1",
    "jquery": "2.2.4",
    "jquery-sidebar": "3.3.2",
    "jquery-ujs": "1.2.0",
    "js-cookie": "2.1.2",
    "leaflet": "0.7.7",
    "lodash": "4.14.2",
    "moment": "2.14.1",
    "react": "15.3.2",
    "react-dom": "15.3.2",
    "react-image-crop": "1.0.0-rc3",
    "react-redux": "4.4.5",
    "redux": "3.5.2",
    "redux-logger": "2.6.1",
    "redux-thunk": "2.1.0",
    "reselect": "2.5.3",
    "showdown": "git+https://github.com/socrata/showdown.git#openperformance-compat",
    "socrata-components": "8.2.0",
    "socrata-utils": "3.1.1",
    "socrata-visualizations": "19.3.3",
    "squire-rte": "git+https://github.com/socrata-platform/Squire.git#1.4.2-fixes",
    "tether": "1.0.1",
    "tether-shepherd": "1.2.0",
    "unidragger": "2.1.0",
    "unipointer": "2.1.0",
    "velocity-animate": "1.2.3",
    "whatwg-fetch": "1.0.0"
  }
}
