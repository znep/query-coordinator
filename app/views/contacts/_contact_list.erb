<table id='contactList' class='gridList selectableList' cellspacing="0">
  <colgroup>
    <col class='handle' />
    <col class='picture' />
    <col class='name' />
    <col class='tags' />
    <col class='activityDate' />
  </colgroup>
  <thead>
    <tr>
      <th class='handle first'><div></div></th>
      <th class='picture' title='Picture'><div>Picture</div></th>
      <th class='name'><div>Name</div></th>
      <th class='tags'><div>Tags</div></th>
      <th class='activityDate last headerSortUp'><div>Last Activity</div></th>
    </tr>
  </thead>
  <tbody>
    <%- @group_heading = nil -%>
    <%= render(:partial => "contact_row", :collection => @contacts) %>
  </tbody>
</table>
