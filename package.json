{
  "name": "socrata-visualizations",
  "version": "18.11.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/socrata/frontend-visualizations.git"
  },
  "publishConfig": {
    "registry": "https://socrata.artifactoryonline.com/socrata/api/npm/npm-local"
  },
  "scripts": {
    "release": "sh tools/release.sh",
    "postinstall": "npm rebuild node-sass",
    "build": "webpack && npm run compile-css",
    "webpack": "webpack",
    "prebuild": "rimraf dist",
    "prepublish": "npm run build",
    "lint": "eslint -c package.json  --ignore-path .eslintignore src/**/*.js",
    "test": "karma start karma.conf.js --singleRun true",
    "test-chrome": "karma start karma.conf.js --singleRun false --browsers Chrome",
    "version-bump-check": "node tools/version-bump-check.js",
    "watch": "karma start karma.conf.js -v --singleRun false",
    "compile-css": "node tools/compile-css.js",
    "compile-css-watch": "node tools/compile-css.js --watch"
  },
  "homepage": "https://github.com/socrata/frontend-visualizations",
  "main": "dist/socrata-visualizations.js",
  "devDependencies": {
    "autoprefixer": "6.3.6",
    "babel-polyfill": "6.16.0",
    "chai": "3.5.0",
    "chai-dom": "1.4.0",
    "chokidar": "1.5.2",
    "eslint": "2.3.0",
    "eslint-base": "0.2.3",
    "karma": "0.13.22",
    "karma-chai": "0.1.0",
    "karma-chai-dom": "1.1.0",
    "karma-chrome-launcher": "0.2.3",
    "karma-coverage": "0.5.5",
    "karma-mocha": "0.2.2",
    "karma-phantomjs-launcher": "1.0.1",
    "karma-phantomjs-shim": "1.4.0",
    "karma-scss-preprocessor": "1.0.1",
    "karma-sinon": "1.0.5",
    "karma-sourcemap-loader": "0.3.7",
    "karma-webpack": "1.7.0",
    "mocha": "2.5.3",
    "node-sass": "3.7.0",
    "phantomjs-prebuilt": "2.1.9",
    "postcss": "5.0.21",
    "react-addons-test-utils": "0.14.6",
    "rewire": "2.5.1",
    "rewire-webpack": "1.0.1",
    "rimraf": "2.5.2",
    "semver": "5.1.0",
    "shelljs": "0.6.0",
    "sinon": "1.17.5",
    "socrata-components": "7.7.2",
    "strict-loader": "1.1.0",
    "webpack": "1.13.1"
  },
  "eslintConfig": {
    "extends": "./node_modules/eslint-base/.eslintrc.json",
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "rules": {
      "dot-notation": [
        1,
        {
          "allowKeywords": true
        }
      ]
    },
    "globals": {
      "ArrayBuffer": true,
      "Promise": true,
      "Uint8Array": true,
      "VBArray": true,
      "VectorTile": true
    }
  },
  "dependencies": {
    "babel": "6.5.2",
    "babel-core": "6.8.0",
    "babel-loader": "6.2.4",
    "babel-preset-es2015": "6.9.0",
    "babel-preset-react": "6.5.0",
    "chroma-js": "0.5.6",
    "classnames": "2.2.5",
    "d3": "3.5.16",
    "jquery": "2.1.1",
    "leaflet": "0.7.7",
    "lodash": "4.13.1",
    "moment": "2.10.6",
    "pbf": "1.3.5",
    "react": "0.14.6",
    "react-dom": "0.14.6",
    "react-dotdotdot": "1.0.16",
    "react-redux": "4.4.5",
    "redux": "3.5.2",
    "redux-logger": "2.6.1",
    "redux-thunk": "2.1.0",
    "reselect": "2.5.1",
    "simple-statistics": "0.8.1",
    "socrata-utils": "3.0.1",
    "vector-tile": "0.1.1",
    "wellknown": "0.4.2",
    "whatwg-fetch": "1.0.0"
  },
  "files": [
    "dist"
  ]
}
