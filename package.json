{
  "name": "socrata-visualizations",
  "version": "7.2.2",
  "repository": {
    "type": "git",
    "url": "https://github.com/socrata/frontend-visualizations.git"
  },
  "publishConfig": {
    "registry": "https://socrata.artifactoryonline.com/socrata/api/npm/npm-local"
  },
  "scripts": {
    "postinstall": "npm rebuild node-sass",
    "build": "webpack && node tools/compile-css.js",
    "prebuild": "rimraf dist",
    "prepublish": "webpack && node tools/compile-css.js",
    "lint": "eslint -c package.json  --ignore-path .eslintignore src/**/*.js",
    "test": "karma start karma.conf.js --singleRun true",
    "watch": "karma start karma.conf.js -v --singleRun false",
    "compile-css": "node tools/compile-css.js"
  },
  "homepage": "https://github.com/socrata/frontend-visualizations",
  "main": "dist/socrata-visualizations.js",
  "devDependencies": {
    "autoprefixer": "^6.3.1",
    "chai": "^3.0.0",
    "eslint": "^2.3.0",
    "eslint-base": "0.2.3",
    "karma": "^0.13.14",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^0.2.1",
    "karma-coverage": "^0.5.3",
    "karma-mocha": "^0.2.1",
    "karma-phantomjs-launcher": "^0.2.0",
    "karma-phantomjs-shim": "^1.1.2",
    "karma-scss-preprocessor": "^1.0.0",
    "karma-sinon": "^1.0.4",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.7.0",
    "mocha": "^2.2.5",
    "node-sass": "^3.3.2",
    "phantomjs": "^1.9.17",
    "postcss": "^5.0.14",
    "rewire": "^2.5.0",
    "rewire-webpack": "^1.0.1",
    "rimraf": "^2.4.3",
    "sinon": "^1.15.3",
    "strict-loader": "^1.0.0",
    "styleguide": "socrata/styleguide.git#0.1.0",
    "webpack": "^1.12.2"
  },
  "eslintConfig": {
    "extends": "./node_modules/eslint-base/.eslintrc.json",
    "rules": {
      "dot-notation": [1, {"allowKeywords": true}]
    },
    "globals": {
      "$": true,
      "_": true,
      "ArrayBuffer": true,
      "chroma": true,
      "d3": true,
      "jQuery": true,
      "L": true,
      "moment": true,
      "pbf": true,
      "Promise": true,
      "socrata": true,
      "ss": true,
      "Uint8Array": true,
      "VBArray": true,
      "VectorTile": true
    }
  },
  "dependencies": {
    "leaflet": "^0.7.3",
    "moment": "~2.10.6",
    "pbf": "^1.3.5",
    "vector-tile": "0.1.1"
  },
  "files": [
    "dist"
  ]
}
