{
  "name": "socrata-visualizations",
  "version": "10.7.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/socrata/frontend-visualizations.git"
  },
  "publishConfig": {
    "registry": "https://socrata.artifactoryonline.com/socrata/api/npm/npm-local"
  },
  "scripts": {
    "postinstall": "npm rebuild node-sass",
    "build": "webpack && npm run compile-css",
    "prebuild": "rimraf dist",
    "prepublish": "npm run build",
    "lint": "eslint -c package.json  --ignore-path .eslintignore src/**/*.js",
    "test": "karma start karma.conf.js --singleRun true",
    "version-bump-check": "node tools/version-bump-check.js",
    "watch": "karma start karma.conf.js -v --singleRun false",
    "compile-css": "node tools/compile-css.js"
  },
  "homepage": "https://github.com/socrata/frontend-visualizations",
  "main": "dist/socrata-visualizations.js",
  "devDependencies": {
    "autoprefixer": "^6.3.1",
    "chai": "^3.0.0",
    "chai-dom": "1.4.0",
    "eslint": "2.3.0",
    "eslint-base": "0.2.3",
    "karma": "^0.13.14",
    "karma-chai": "^0.1.0",
    "karma-chai-dom": "1.1.0",
    "karma-chrome-launcher": "^0.2.1",
    "karma-coverage": "^0.5.3",
    "karma-mocha": "^0.2.1",
    "karma-phantomjs-launcher": "^0.2.0",
    "karma-phantomjs-shim": "^1.1.2",
    "karma-scss-preprocessor": "^1.0.0",
    "karma-sinon": "^1.0.4",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.7.0",
    "mocha": "^2.2.5",
    "node-sass": "^3.4.2",
    "phantomjs": "^1.9.17",
    "postcss": "^5.0.14",
    "react-addons-test-utils": "0.14.6",
    "rewire": "^2.5.0",
    "rewire-webpack": "^1.0.1",
    "rimraf": "^2.4.3",
    "semver": "^5.1.0",
    "shelljs": "^0.6.0",
    "sinon": "^1.15.3",
    "socrata-styleguide": "2.0.0",
    "strict-loader": "^1.0.0",
    "webpack": "^1.12.2"
  },
  "eslintConfig": {
    "extends": "./node_modules/eslint-base/.eslintrc.json",
    "rules": {
      "dot-notation": [
        1,
        {
          "allowKeywords": true
        }
      ]
    },
    "globals": {
      "$": true,
      "_": true,
      "ArrayBuffer": true,
      "chroma": true,
      "d3": true,
      "jQuery": true,
      "L": true,
      "moment": true,
      "pbf": true,
      "Promise": true,
      "React": true,
      "ReactDOM": true,
      "socrata": true,
      "ss": true,
      "Uint8Array": true,
      "VBArray": true,
      "VectorTile": true
    }
  },
  "dependencies": {
    "babel": "6.5.2",
    "babel-core": "6.8.0",
    "babel-loader": "6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "6.5.0",
    "chroma-js": "0.5.6",
    "d3": "3.5.16",
    "jquery": "2.1.1",
    "leaflet": "0.7.7",
    "lodash": "3.10.0",
    "moment": "2.10.6",
    "pbf": "1.3.5",
    "react": "0.14.6",
    "react-dom": "0.14.6",
    "react-redux": "4.4.5",
    "redux": "3.5.2",
    "redux-logger": "^2.6.1",
    "redux-thunk": "^2.0.1",
    "simple-statistics": "0.8.1",
    "socrata-utils": "1.3.0",
    "vector-tile": "0.1.1"
  },
  "files": [
    "dist"
  ]
}
