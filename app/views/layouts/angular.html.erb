<!doctype html>
<html>
  <head>
    <meta http-equiv="content-type" content="text/html;charset=utf-8" />
    <%= rendered_stylesheet_tag 'bower-styles-all' %>
    <%= rendered_stylesheet_tag 'angular-app-'+params['app'] %>
    <%= yield :head %>
  </head>
  <body ng-app="<%= params['app']%>">
    <%= render params['app'] %>
    <%= yield :layout %>
  </body>
</html>
