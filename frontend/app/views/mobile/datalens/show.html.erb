<%= render :partial => 'mobile/datalens/mobile_navbar' %>
<div>
  <%= render :partial => 'mobile/datalens/mobile_meta' %>
  <div id="filters"></div>
  <div id="mobile-components"></div>
</div>

<div id="modal-container" class="hidden">
  <div class="warning-modal">
    <i id="btn-close" class="icon-close"></i>
    <h1>Heads Up!</h1>
    <p>
      Looks like all the data has been filtered out on this page.<br>
      You might want to remove the filters before continuing.
    </p>
    <div class="warning-modal-button-container">
      <span id="btn-proceed" class="btn btn-text" href="#">Proceed anyway</span>
      <span id="btn-clear-filters" class="btn btn-primary" href="#">Clear Filters</span>
    </div>
  </div>
</div>
