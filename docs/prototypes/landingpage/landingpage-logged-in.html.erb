    }

    else {
      $('#featured-item-2 .actual-content').hide();
      $('.url-error').show();
      $('#add-external-button').prop('disabled', true);
    }
  })
}, 5000);


//show text in preview
$(function() {
  $('#ext-title-input').keyup(function() {
    $('#ext-title-preview, #ext-asset-title').text($(this).val());
  });
});
$(function() {
  $('#ext-description-input').keyup(function() {
    $('#ext-description-preview, #ext-asset-description').text($(this).val());
  });
});


//display img upload
function readURL(input) {

    if (input.files && input.files[0]) {
        var reader = new FileReader();

        reader.onload = function (e) {
            $('#image-preview, #ext-asset-image').attr('src', e.target.result);
        }

        reader.readAsDataURL(input.files[0]);
    }
}

$("#image-file").change(function(){
    readURL(this);
});



//tabs logic
$(document).ready(function(){
   $('ul.nav-tabs li').click(function(){
    var tab_id = $(this).attr('data-tab-id');

    $('ul.nav-tabs li').removeClass('current');
    $('.tab-content').removeClass('current');

    $(this).addClass('current');
    $("#"+tab_id).addClass('current');
  })
});

</script>


