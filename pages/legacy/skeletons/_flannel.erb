<div id="<%= flannel_id %>" class="flannel flannel-hidden">
  <header class="flannel-header">
    <h1 class="flannel-header-title h5">Flannel Header</h1>
    <button class="btn btn-transparent flannel-header-dismiss" data-flannel-dismiss>
      <span class="icon-close-2"></span>
    </button>
  </header>
  <section class="flannel-content">
    <p>Cow alcatra beef ribs short loin, sausage landjaeger jerky sirloin rump turducken.</p>
  </section>
  <footer class="flannel-actions">
      <button class="btn btn-default btn-xs" data-flannel-dismiss>Cancel</button>
      <button class="btn btn-primary btn-xs" data-flannel-dismiss>OK</button>
  </footer>
</div>
