{
  "name": "socrata-frontend",
  "version": "0.0.0",
  "description": "Socrata's default UX.",
  "main": "",
  "directories": {
    "test": "test"
  },
  "config": {
    "source": "public/javascripts/src",
    "destination": "public/javascripts/dist",
    "webpackConfig": "config/webpack.config.js",
    "webpackPublicPath": "javascripts/build",
    "webpackDestination": "public/javascripts/build",
    "webpackDevServerPort": "3030",
    "datalensWebpackExternals": {
      "angular": "angular",
      "rx": "Rx",
      "jQuery": "$",
      "d3": "d3",
      "_": "_",
      "socrata.utils": "socrata.utils"
    }
  },
  "scripts": {
    "build:dev": "webpack --config ${npm_package_config_webpackConfig}",
    "prebuild:dev": "npm run webpack-clean",
    "build:prod": "NODE_ENV=production webpack --config ${npm_package_config_webpackConfig}",
    "prebuild:prod": "npm run webpack-clean",
    "webpack-clean": "rimraf ${npm_package_config_webpackDestination}",
    "webpack-dev-server": "webpack-dev-server --config ${npm_package_config_webpackConfig} --https --cert dev-server/ssl/host.crt --cacert dev-server/ssl/ca.crt --key dev-server/ssl/host.key",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint"
  },
  "repository": {
    "type": "git",
    "url": "git@git.socrata.com:frontend.git"
  },
  "author": "Socrata Engineering <engineering@socrata.com>",
  "license": "all rights reserved",
  "devDependencies": {
    "babel": "^6.0.15",
    "babel-core": "^6.1.4",
    "babel-eslint": "^4.1.5",
    "babel-loader": "^6.1.0",
    "babel-plugin-transform-object-rest-spread": "^6.1.4",
    "babel-preset-es2015": "^6.1.4",
    "babel-preset-react": "^6.1.4",
    "chai": "~1.9.1",
    "chai-as-promised": "~4.1.1",
    "eslint": "^1.8.0",
    "eslint-config-angular": "^0.4.0",
    "eslint-config-airbnb": "^1.0.0",
    "eslint-loader": "^1.1.1",
    "eslint-plugin-angular": "^0.13.0",
    "eslint-plugin-react": "^3.7.1",
    "exports-loader": "^0.6.2",
    "html-loader": "^0.3.0",
    "http-proxy": "^1.1.6",
    "imports-loader": "^0.6.5",
    "karma": "~0.13.14",
    "karma-chai": "~0.1.0",
    "karma-chai-plugins": "~0.2.4",
    "karma-chrome-launcher": "~0.1.3",
    "karma-cli": "0.0.4",
    "karma-coverage": "~0.2.1",
    "karma-firefox-launcher": "~0.1.3",
    "karma-mocha": "~0.1.3",
    "karma-mocha-reporter": "~0.2.7",
    "karma-ng-html2js-preprocessor": "~0.1",
    "karma-phantomjs-launcher": "^0.2.0",
    "karma-sauce-launcher": "^0.2.10",
    "karma-webpack": "^1.7.0",
    "mocha": "~1.18.2",
    "ng-annotate-loader": "0.0.10",
    "ngtemplate-loader": "^1.3.1",
    "phantomjs": "^1.9.17",
    "raw-loader": "^0.5.1",
    "react-addons-test-utils": "^0.14.2",
    "react-hot-loader": "^1.3.0",
    "rimraf": "^2.4.3",
    "script-loader": "^0.6.1",
    "sprintf": "^0.1.4",
    "webpack": "^1.12.2",
    "webpack-dev-server": "^1.12.1",
    "webpack-manifest-plugin": "^0.4.0"
  },
  "dependencies": {
    "lodash": "^3.10.1",
    "react": "^0.14.2",
    "react-dom": "^0.14.2",
    "socrata-visualizations": "^3.0.3"
  }
}
