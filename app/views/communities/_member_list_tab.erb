
<div class="tabContentOuter"><div class="tabContentTL"><div class="tabContentBL">
  <div class="tabContent">
    <h2><%= tab_title %></h2>
    <div class="contentSort">
      <% form_tag(filter_community_url(:type => type, :filter => current_filter), :class => "sortForm") do %>
        <p>
          Sort by
          <%= select_tag("sort_by", community_sort_select_options(sort_by), :id => "sort_by_" + type) %>
        </p>
      <% end %>
    </div>
  
    <ul class="communityContentList">
      <%= render(:partial => "member_list_item", :collection => members) %>
    </ul>
    
    <div class="memberPager">
      <%= create_pagination(members_total, @page_size, current_page, filter_community_url({:type => type, :filter => current_filter, :sort_by => sort_by})) %>
    </div>
    
  </div>
</div></div></div>

<div class="tabContentNavTR"><div class="tabContentNavBR">
  <div class="tabContentNav">
    <h2>Show People</h2>
    <ul class="tagList">
      <li><%= link_to("With public #{t(:blists_name)}", 
              filter_community_url({:type => type, :filter => {:publicOnly => true}}),
              :class => "filterLink") %></li>
    </ul>
    
    <h2>Browse by tag</h2>
    <ul class="tagList">
      <li><a href="#tag">Wines</a></li>
      <li><a href="#tag">Contributors</a></li>
      <li><a href="#tag">Energy</a></li>
      <li><a href="#tag">Finance</a></li>
    </ul>
  </div>
</div></div>