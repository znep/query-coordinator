{
  "name": "common-code-tests",
  "description": "Tests for code in common",
  "version": "0.0.0",
  "license": "UNLICENSED",
  "scripts": {
    "build": "webpack",
    "prebuild": "rimraf dist",
    "check-dependencies": "check-dependencies",
    "lint": "eslint -c package.json ../",
    "test": "karma start karma.conf.js --singleRun true",
    "watch": "karma start karma.conf.js -v --singleRun false"
  },
  "dependencies": {
    "es6-promise-polyfill": "1.2.0"
  },
  "devDependencies": {
    "babel-cli": "6.4.0",
    "babel-core": "6.10.4",
    "babel-eslint": "6.0.4",
    "babel-loader": "6.2.1",
    "babel-plugin-transform-object-rest-spread": "6.3.13",
    "babel-preset-es2015": "6.3.13",
    "chai": "3.5.0",
    "check-dependencies": "1.0.1",
    "eslint": "2.3.0",
    "eslint-base": "0.2.3",
    "eslint-plugin-import": "1.16.0",
    "jquery": "2.2.4",
    "karma": "0.13.22",
    "karma-chai": "0.1.0",
    "karma-coverage": "0.5.5",
    "karma-mocha": "0.2.2",
    "karma-phantomjs-launcher": "0.2.3",
    "karma-phantomjs-shim": "1.4.0",
    "karma-sinon": "1.0.5",
    "karma-sourcemap-loader": "0.3.7",
    "karma-webpack": "2.0.2",
    "lodash": "4.14.2",
    "mocha": "2.5.3",
    "phantomjs": "1.9.20",
    "rimraf": "2.5.2",
    "semver": "5.1.0",
    "shelljs": "0.6.0",
    "sinon": "1.17.4",
    "webpack": "2.2.1"
  },
  "peerDependencies": {
    "lodash": "4.14.2",
    "jquery": "2.2.4"
  },
  "eslintConfig": {
    "extends": "./node_modules/eslint-base/.eslintrc.json",
    "rules": {
      "dot-notation": [
        1,
        {
          "allowKeywords": true,
          "allowPattern": "^[a-z]+(_[a-z]+)+$"
        }
      ]
    },
    "globals": {
      "$": true,
      "_": true,
      "jQuery": true,
      "L": true,
      "moment": true,
      "pbf": true,
      "socrata": true,
      "ss": true,
      "require": true,
      "module": true
    }
  }
}
