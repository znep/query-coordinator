<% cache(cache_key_for_site_chrome(get_site_chrome)) do %>
  @import url("/socrata_site_chrome/stylesheets/mono_social_icons_font/monosocialiconsfont.css");
  <% unless exclude_styleguide? %>
    <%# Note: excluding the actual styleguide CSS for now, because it doesn't currently play nicely
      # with many of the pages we want to use the header/footer on.
      # When that changes, we will want to remove the two font import statements below and only use
      # @import url("/stylesheets/styleguide/css/styleguide.min.css");
    %>
    @import url("/socrata_site_chrome/stylesheets/styleguide/fonts/socrata-icons-font-family.css");
    @import url("/socrata_site_chrome/stylesheets/styleguide/fonts/socrata-icons.css");
  <% end %>
  <% if params.key?(:debug_sass) %>
///////////////////////
//   Unparsed SASS   //
///////////////////////

<%== render('theme', :custom_themes => get_site_chrome.styles) %>
  <% else %>
<%== Sass::Engine.new(render('theme', :custom_themes => get_site_chrome.styles), sass_engine_options).render %>
  <% end %>
<% end %>
