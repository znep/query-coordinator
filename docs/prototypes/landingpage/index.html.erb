<script src="https://ajax.googleapis.com/ajax/libs/jquery/2.1.4/jquery.min.js"></script>
<script src="https://www.google.com/recaptcha/api.js" async defer></script>

<script>

  var viewPortHeight = $(window).height();
  var headerHeight = $(".menu-header").outerHeight(true);
  var filterActionHeight = $(".filter-action-container").outerHeight(true);
  var filterListActveHeight = $(".filter-list.active").outerHeight(true);
  var maxContentHeight = viewPortHeight - headerHeight - filterActionHeight - filterListActveHeight + 63;



  $("#launch-congrats-modal").on("click", function(){
    $("#contact-modal").addClass('modal-hidden');
    //$("#congrats-modal").removeClass('modal-hidden');
    $("#congrats-modal").removeClass("modal-hidden").delay(2000).fadeOut(400);
  })


  //hide the expandable details
  $("tr .row").hide(); //div
  $("tr.details-row").hide(); //tr


  //animations for schema preview
  $(".schema-preview tr.no-details-row").on("click", function(e) {

      //if the details aren't shown
      if($(this).next("tr.details-row").is(":visible") == false) {

        //show the <tr> then animate
        $(this).next().show();
        $(this).next("tr").children().children(".row").slideToggle("slow");

      } else {

          //animate then hide the <tr>
          $(this).next("tr").children().children(".row").slideToggle("slow", function() {
            $(this).parent().parent().hide();

          });
      }

      $(this).toggleClass("expanded");

   })
  $(".alert .icon-close-2").on("click", function() {
    $(".alert").hide();
  })

  // Set the height of the menu content area to position the button
  $(".filter-wrapper").css("height", maxContentHeight);

  console.log("View Port: " + viewPortHeight);
  console.log("Header: " + headerHeight);
  console.log("Filter Action Button: " + filterActionHeight);
  console.log("Active List: " + filterListActveHeight);
  console.log("Content Max: " + maxContentHeight);

  // Hide the facet list & the hint text
  $(".filter-list ul, .filter-hint-text").hide();


  //Toggle the facet group
  $(".facet-group").each(function() {
    $(this).show(0).on("click", function(e){
      e.preventDefault();
      $(this).toggleClass("open");
      $(this).next("ul").slideToggle("fast");
    })
  })

  $(".facet-item a").each(function() {
    $(this).on("click", function(e) {
      e.preventDefault();
      $(this).parent("li").siblings("li").find("a").removeClass("active");
      $(this).toggleClass("active");
    })
  })

  //Collect and display current filters, remove them, do the filter
  $("#filter").on("click", function(e) {

    $(".active-filters a.active").remove();

    $(".filter-list.active ul, .filter-hint-text").show();

    $(".facet-group.open").next("ul").slideToggle();
    $(".facet-group").removeClass("open");

    var selectedFilters = $(".facet-item a.active:not(.filter-list.active a.active)").get();
    //console.log(selectedFilters);

    $.each(selectedFilters, function(i, obj) {

      var facetGroupName = $(obj).parents("ul").attr("data-facet-name");
     // console.log(facetGroupName);

      $(".active-filters").append("<li class='facet-item'><a class='active' href='#'>" + facetGroupName + " > " + obj.innerHTML + "</a></li>");
    })

    $(".active-filters a.active").on("click", function(e) {
        $(this).remove();
    })

    $("#clear-filters").on("click", function(e) {
      $(".active-filters a.active").remove();
      $(".facet-item a.active").toggleClass("active");
      $(".facet-group.open").next("ul").slideToggle();
      $(".facet-group").removeClass("open");
      $(".filter-hint-text").hide();
    })

  });

</script>


